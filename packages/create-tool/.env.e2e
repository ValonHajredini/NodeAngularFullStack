# E2E Test Environment Configuration
# Used by Jest E2E tests for create-tool CLI

# Database Configuration
DATABASE_URL=postgresql://dbuser:dbpassword@localhost:5432/nodeangularfullstack
DB_HOST=localhost
DB_PORT=5432
DB_NAME=nodeangularfullstack
DB_USER=dbuser
DB_PASSWORD=dbpassword

# API Configuration
API_BASE_URL=http://localhost:3000

# Admin Credentials for Tool Registration
# These credentials are created by the E2E setup script
CREATE_TOOL_ADMIN_EMAIL=e2e-admin@example.com
CREATE_TOOL_ADMIN_PASSWORD=E2eAdmin123!@#

# CLI Workspace Configuration
CREATE_TOOL_WORKSPACE=/tmp/create-tool-e2e

# Timeouts (in milliseconds)
CLI_TIMEOUT=60000
API_TIMEOUT=30000

# Test Mode
NODE_ENV=test

# Development Environment Configuration
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:4200

# Database (Development)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=nodeangularfullstack
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_SSL=false

# JWT Authentication (Development - Use secure values in production)
JWT_SECRET=development-jwt-secret-key-at-least-32-characters-long
JWT_REFRESH_SECRET=development-refresh-secret-key-at-least-32-characters-long
JWT_EXPIRES_IN=3600
JWT_REFRESH_EXPIRES_IN=604800

# Redis Cache (Development)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# File Storage (Development)
DO_SPACES_ENDPOINT=https://nyc3.digitaloceanspaces.com
DO_SPACES_KEY=your-spaces-key
DO_SPACES_SECRET=your-spaces-secret
DO_SPACES_BUCKET=your-bucket-name
DO_SPACES_REGION=nyc3

# Email Service (Development)
SENDGRID_API_KEY=your-sendgrid-api-key
EMAIL_FROM=dev@localhost.com
EMAIL_FROM_NAME=NodeAngularFullStack Dev

# Monitoring (Development - Optional)
SENTRY_DSN=
LOGTAIL_TOKEN=

# Rate Limiting (Development - More permissive)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# CORS Origins (Development)
CORS_ORIGINS=http://localhost:4200,http://localhost:3000

# API Keys (Development)
API_KEY=development-api-key

# Multi-tenancy Configuration (Development - Single Tenant Mode)
ENABLE_MULTI_TENANCY=false
DEFAULT_TENANT_ID=development

# Tenant Isolation Settings (Optimized for Single Tenant)
TENANT_ISOLATION_LEVEL=row
TENANT_RLS_ENABLED=false
TENANT_CROSS_ACCESS_PREVENTION=false

# Tenant Security Settings (Minimal for Development)
TENANT_AUDIT_LOGGING=false
TENANT_TOKEN_ISOLATION=false
TENANT_DATA_ENCRYPTION=false
TENANT_REQUIRE_VERIFICATION=false

# Tenant Defaults (Used when switching to multi-tenancy)
TENANT_DEFAULT_PLAN=free
TENANT_MAX_USERS_DEFAULT=10
TENANT_FEATURES_DEFAULT={}

# pgWeb Database Management (Development)
PGWEB_AUTH_USER=admin
PGWEB_AUTH_PASS=development-password
PGWEB_READ_ONLY=false
PGWEB_SESSIONS=true
PGWEB_CORS_ORIGIN=http://localhost:4200
PGWEB_DATABASE_URL=postgresql://your_db_user:your_db_password@postgres:5432/nodeangularfullstack?sslmode=disable